{
  "version": 1,
  "projects": {
    "client-app": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "projectType": "application",
      "root": "apps/client-app",
      "sourceRoot": "apps/client-app/src",
      "prefix": "ngrx-transactions-app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/client-app",
            "index": "apps/client-app/src/index.html",
            "main": "apps/client-app/src/main.ts",
            "polyfills": "apps/client-app/src/pollyfills.ts",
            "tsConfig": "apps/client-app/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/client-app/src/favicon.ico",
              "apps/client-app/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/client-app/src/styles.scss",
              "apps/client-app/src/theme.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/client-app/src/environments/environment.ts",
                  "with": "apps/client-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "client-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "client-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "client-app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/client-app/src/**/*.ts",
              "apps/client-app/src/**/*.html"
            ]
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {}
        }
      },
      "tags": []
    },
    "core-guards": {
      "projectType": "library",
      "root": "libs/core/src/lib/guards",
      "sourceRoot": "libs/core/src/lib/guards/src",
      "prefix": "ngrx-transactions-app",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/core/src/lib/guards/**/*.ts",
              "libs/core/src/lib/guards/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "dashboard": {
      "projectType": "library",
      "root": "libs/dashboard",
      "sourceRoot": "libs/dashboard/src",
      "prefix": "ngrx-transactions-app",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/dashboard/**/*.ts",
              "libs/dashboard/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "interfaces": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "root": "libs/interfaces",
      "sourceRoot": "libs/interfaces/src",
      "prefix": "ngrx-transactions-app",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/interfaces/**/*.ts"
            ]
          }
        }
      },
      "tags": []
    },
    "core-forms": {
      "projectType": "library",
      "sourceRoot": "libs/core/src/lib/components/src/lib/form/src",
      "prefix": "ngrx-transactions-app",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/core/src/lib/components/src/lib/form/**/*.ts",
              "libs/core/src/lib/components/src/lib/form/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "core-models": {
      "projectType": "library",
      "root": "libs/core/src/lib/models",
      "sourceRoot": "libs/core/src/lib/models/src",
      "prefix": "ngrx-transactions-app",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "outputs": [
            "dist/libs/core/src/lib/models"
          ],
          "options": {
            "project": "libs/core/src/lib/models/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/core/src/lib/models/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/core/src/lib/models/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/core/src/lib/models/**/*.ts",
              "libs/core/src/lib/models/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "shopping-cart": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "projectType": "library",
      "root": "libs/shopping-cart",
      "sourceRoot": "libs/shopping-cart/src",
      "prefix": "vaki-challenge",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shopping-cart/src/**/*.ts",
              "libs/shopping-cart/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "ui": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "projectType": "library",
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "prefix": "vaki-challenge",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui/src/**/*.ts",
              "libs/ui/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    }
  },
  "defaultProject": "client-app"
}