{
  "version": 1,
  "projects": {
    "models": {
      "projectType": "library",
      "root": "libs/core/src/lib/models",
      "sourceRoot": "libs/core/src/lib/models/src",
      "prefix": "ngrx-transactions-app",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "outputs": [
            "dist/libs/core/src/lib/models"
          ],
          "options": {
            "project": "libs/core/src/lib/models/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/core/src/lib/models/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/core/src/lib/models/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/core/src/lib/models/**/*.ts",
              "libs/core/src/lib/models/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "interfaces": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "root": "libs/interfaces",
      "sourceRoot": "libs/interfaces/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/interfaces/**/*.ts"
            ]
          }
        }
      },
      "tags": []
    },
    "shopping-cart": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "projectType": "library",
      "root": "libs/shopping-cart",
      "sourceRoot": "libs/shopping-cart/src",
      "prefix": "vaki-challenge",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shopping-cart/src/**/*.ts",
              "libs/shopping-cart/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "ui": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "projectType": "library",
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "prefix": "vaki-challenge",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui/src/**/*.ts",
              "libs/ui/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "vakers": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "projectType": "application",
      "root": "apps/vakers",
      "sourceRoot": "apps/vakers/src",
      "prefix": "vaki-challenge",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/vakers",
            "index": "apps/vakers/src/index.html",
            "main": "apps/vakers/src/main.ts",
            "polyfills": "apps/vakers/src/polyfills.ts",
            "tsConfig": "apps/vakers/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/vakers/src/favicon.ico",
              "apps/vakers/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/vakers/src/styles.scss",
              "apps/vakers/src/theme.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/vakers/src/environments/environment.ts",
                  "with": "apps/vakers/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "vakers:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "vakers:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "vakers:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/vakers/src/**/*.ts",
              "apps/vakers/src/**/*.html"
            ]
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {}
        }
      },
      "tags": []
    },
    "vakers-e2e": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "root": "apps/vakers-e2e",
      "sourceRoot": "apps/vakers-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/vakers-e2e/cypress.json",
            "tsConfig": "apps/vakers-e2e/tsconfig.e2e.json",
            "devServerTarget": "vakers:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "vakers:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/vakers-e2e/**/*.{js,ts}"
            ]
          }
        }
      },
      "tags": [],
      "implicitDependencies": [
        "vakers"
      ]
    }
  }
}
